include userBlock
      
mixin memoryBlock(memory={})
  .memoryWrapper
    a(href=`/memory/${memory._id}`)
      .memory
        img(src=`/${memory.thumbnail}`)
        h3=memory.title
    if memory.creator
      +userBlock({
        avatar:memory.creator.avatar,
        name:memory.creator.name,
        _id:memory.creator._id
      })
    